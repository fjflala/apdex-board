{
  "presets": [
    [
      "@babel/react",
      {
        "pragma": "Dom.createElement", // 
        "pragmaFrag": "DomFrag", // 
        "throwIfNamespace": false // 
      }
    ],
    [
  	  "env", 
      {
        "targets": {
          "browsers": [
            "last 2 versions", 
            "not ie < 11"
          ]
        }
      }
    ],
  ],
  "plugins": ["@babel/plugin-syntax-jsx"]
}